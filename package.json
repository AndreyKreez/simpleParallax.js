{
    "name": "simple-parallax-js",
    "version": "4.2.1",
    "description": "simpleParallax is a simple and lightweight JS plugin that gives your website parallax animations on the images",
    "homepage": "https://simpleparallax.com/",
    "main": "dist/simpleParallax.min.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/geosenna/simpleParallax.git"
    },
    "keywords": [
        "parallax",
        "scroll",
        "simple",
        "easy",
        "fast",
        "light",
        "image",
        "effect",
        "vanilla",
        "es6"
    ],
    "author": "Geoffrey Signorato <geoffrey.signorato@gmail.com>",
    "license": "MIT",
    "scripts": {
        "start": "webpack-dev-server --mode development --open",
        "build": "webpack --mode production",
        "format": "prettier --write '**/**.{js,json,html}'",
        "lint": "npx eslint ./src/simpleParallax.js"
    },
    "bugs": {
        "url": "https://github.com/geosenna/simpleParallax/issues"
    },
    "dependencies": {
        "@babel/core": "^7.4.0",
        "@babel/preset-env": "^7.4.2",
        "babel-loader": "^8.0.5",
        "html-webpack-plugin": "^3.2.0",
        "uglifyjs-webpack-plugin": "^2.1.2",
        "webpack": "^4.29.6",
        "webpack-cli": "^3.3.0",
        "webpack-dev-server": "^3.2.1"
    },
    "devDependencies": {
        "eslint": "^5.16.0",
        "prettier": "1.16.4"
    }
}
