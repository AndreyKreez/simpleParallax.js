{
    "name": "simple-parallax-js",
    "version": "5.0.0alpha",
    "description": "simpleParallax is a simple and lightweight JS plugin that gives your website parallax animations on images",
    "homepage": "https://simpleparallax.com/",
    "main": "dist/simpleParallax.min.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/geosigno/simpleParallax.git"
    },
    "keywords": [
        "parallax",
        "scroll",
        "simple",
        "easy",
        "fast",
        "light",
        "image",
        "effect",
        "vanilla",
        "es6"
    ],
    "author": "Geoffrey Signorato <geoffrey.signorato@gmail.com>",
    "license": "MIT",
    "scripts": {
        "start": "webpack-dev-server --mode development --open",
        "build": "webpack --mode production",
        "format": "prettier --write '**/**.{js,json,html}'",
        "lint": "npx eslint ./src/**/*.js",
        "test": "jest"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "**/**.{js,json,html}": [
            "prettier --write",
            "git add"
        ]
    },
    "bugs": {
        "url": "https://github.com/geosigno/simpleParallax/issues"
    },
    "dependencies": {
        "@babel/core": "^7.4.4",
        "@babel/preset-env": "^7.4.4",
        "babel-loader": "^8.0.6",
        "html-webpack-plugin": "^3.2.0",
        "webpack": "^4.31.0",
        "webpack-cli": "^3.3.2",
        "webpack-dev-server": "^3.3.1"
    },
    "devDependencies": {
        "eslint": "^5.16.0",
        "husky": "^2.3.0",
        "jest": "^24.8.0",
        "lint-staged": "^8.1.7",
        "moment": "^2.24.0",
        "prettier": "1.16.4",
        "uglifyjs-webpack-plugin": "^2.1.2"
    }
}
